<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!-- Use module for web interface answering -->
    <mode>web</mode>
    <!-- verbose mode on/off -->
    <verbose>on</verbose>
    <!-- logging  on = enable  off = disable-->
    <logging>off</logging>
    <!-- Paths of interpreters for evaluator usage -->
    <evaluator_paths>
        <!-- Path of python 3 executable file -->
        <python3path>
            python3.2
        </python3path>
    </evaluator_paths>
    <!-- Paths of interpreters for internal usage -->
    <internal_paths>
        <!-- Path of python 3 executable file -->
        <python3path>
            python3.2
        </python3path>
    </internal_paths>
    <!-- Folder path for general output files -->
    <outputpath>
        /homes/eva/xb/xbamba01/src/examples/output
    </outputpath>
    <!-- Students list -->
    <students>
        /homes/eva/xb/xbamba01/src/examples/students.csv
    </students>
    <tasks>
        <!-- path with defined tasks, 2 points for task, 5 tasks per student -->
        <taskspath points="2" number="5">
           /homes/eva/xb/xbamba01/src/examples/tasks/program
        </taskspath>
        <!-- path with defined tasks, 2 points for task, 5 tasks per student -->
        <taskspath points="2" number="5">
           /homes/eva/xb/xbamba01/src/examples/tasks/choice
        </taskspath>
        <!-- path with defined tasks, 2 points for task, 5 tasks per student -->
        <taskspath points="2" number="5">
           /homes/eva/xb/xbamba01/src/examples/tasks/multichoice
        </taskspath>
        <!-- path with defined tasks, 2 points for task, 5 tasks per student -->
        <taskspath points="2" number="5">
           /homes/eva/xb/xbamba01/src/examples/tasks/fulltext
        </taskspath>
        <!-- path with defined tasks, 2 points for task, 5 tasks per student -->
        <taskspath points="2" number="5">
           /homes/eva/xb/xbamba01/src/examples/tasks/dynamic
        </taskspath>
    </tasks>
    <webface>
        <port>8866</port>
        <commonpassword>someexample</commonpassword>
        <!-- evaluator strictness: 0: ignore whitespace 1: strict -->
        <eval_strict>0</eval_strict>
        <!-- evaluator timeout for program tasks (in seconds) -->
        <eval_timeout>10</eval_timeout>
        <!-- anonymous mode   on = enable  off = disable-->
        <anonymous>off</anonymous>
        <!-- self learning mode   on = enable  off = disable-->
        <self_learning>off</self_learning>
    </webface>
</config>
